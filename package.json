{
  "name": "vite-chain",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm:fake:backend\" \"npm:dev:frontend\" ",
    "dev:frontend": "vite",
    "dev:backend": "ts-node-dev --respawn --project backend/tsconfig.json backend/main.ts",
    "fake:backend": "ts-node-dev --respawn --project backend/tsconfig.json backend/faker.ts",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^2.0.1",
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "bs58check": "^2.1.2",
    "chalk": "^4",
    "cors": "^2.8.5",
    "elliptic": "^6.5.4",
    "express": "^4.17.3",
    "table": "^6.8.0",
    "timeago.js": "^4.0.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/bs58check": "^2.1.0",
    "@types/cors": "^2.8.12",
    "@types/elliptic": "^6.4.14",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/jest": "^27.4.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "concurrently": "^7.1.0",
    "ts-node-dev": "^1.1.8",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
